{
    "manifest_version": 2,
    "name": "Spat_nav_evaluator 2.0",
    "description": "evaluator for spatial navagiation",
    "version": "2.0",
    "devtools_page": "devtools.html",
    "background": {
       "scripts": ["background.js"],
       "persistent": false
    },
    "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file:///*"],
      "js": ["spatnav-heuristic.js","jquery-3.3.1.min.js","Marker.js", "spat_nav_modules/graph.js"],
      "css": ["Marker.css"],
      "run_at": "document_end",
      "all_frames": true
    }
     ],
    "permissions": [
"*://*/*", "activeTab",  "tabs", "http://*/*", "https://*/*", "file:///*", "storage"
    ]
  }
